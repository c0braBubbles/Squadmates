		<!----------- Loader ----------->
        <div class="loader-wrapper" id="loader">
            <span class="loader"><span class="loader-inner"></span></span>
        </div>

		<div id="subDiv" style="display: none;">
			<%- include('partials/new-groupp'); %>
		</div>


		<script src="js/allgroups.js" type="module"></script>
