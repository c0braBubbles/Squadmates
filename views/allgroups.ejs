		<script type="text/javascript" src="css/bootstrap-5.1.3-dist/js/bootstrap.min.js"></script>
		<script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"></script>
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
		<script src="js/action.js"></script>

		<script>
			//Funksjon for å åpne gruppesid med key lagret
			function getGroup(key) {
				// templating for når man åpner en gruppe: 
				window.open(key, '_self');
				$.post("/openGroup", {
					key: key
				}, function (data, status) {
					console.log(data);
				}
				);

			}
			//Funksjon for å åpne gruppeside med key lagret
			function getPlatform(platform) {
				// templating for når man åpner en platform gruppe: 
				window.open(platform, '_self');
				$.post("/openPlatform", {
					platform: platform
				}, function (data, status) {
					console.log(data);
				}
				);

			}
		</script>

</body>

</html>